# MediPlus Backend Environment Configuration
# Copy this file to .env and fill in your values

# Server Configuration
HOST=0.0.0.0
PORT=5000
DEBUG=True

# Groq API Key for AI-enhanced responses
# Get your key at: https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# Optional: Intel Optimization
# Set to 'true' to enable Intel Extension for PyTorch
INTEL_OPTIMIZATION=false

# Optional: OpenVINO Device
# Options: CPU, GPU, AUTO
OPENVINO_DEVICE=CPU
